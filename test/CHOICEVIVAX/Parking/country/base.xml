<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<om:scenario xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Vivax test" schemaVersion="32" xsi:schemaLocation="http://openmalaria.org/schema/scenario_32 scenario_32.xsd" xmlns:om="http://openmalaria.org/schema/scenario_32">
<!-- make sure the xsd in your directory matches the name here, e.g. scenario_32RC3.xsd in windows or scenario_32.xsd in linux-->
  <interventions name="Full Set">
    <human>
      <deployment name="Extended ITNs">
        <component id="ITN"/>
        <timed>
			<deploy coverage="0.0" maxAge="5" minAge="1" time="7299"/>
        </timed>
      </deployment>
    </human>
	<changeHS>
      <timedDeployment time="7080">
        <ImmediateOutcomes name="Perfect drugs, varying access" useDiagnosticUC="true">
		  <drugRegimen firstLine="ACT" inpatient="QN" secondLine="ACT"/>
		  <initialACR>
			<ACT value="1"/><!-- what is cure rate of blood-stage drugs? -->
			<QN value="1"/><!-- NEW: set to 1 OLD: no treatment for severe (can change so long as CFR=1) -->
			<selfTreatment value="0"/>
		  </initialACR>
		  <compliance>
			<ACT value="1"/>
			<QN value="1"/>
			<selfTreatment value="0"/>
		  </compliance>
		  <nonCompliersEffective>
			<ACT value="0"/>
			<QN value="0"/>
			<selfTreatment value="0"/>
		  </nonCompliersEffective>
		  <treatmentActions>
			<ACT name="clear blood-stage infections">
			  <clearInfections stage="blood" timesteps="-1"/>
			</ACT>
			<QN name="clear blood-stage infections">
			  <clearInfections stage="blood" timesteps="-1"/>
			</QN>
		  </treatmentActions>
		  <!--set as desired... -->
		  <pSeekOfficialCareUncomplicated1 value="0.08"/>
		  <pSelfTreatUncomplicated value="0"/>
		  <pSeekOfficialCareUncomplicated2 value="0.08"/>
		  <pSeekOfficialCareSevere value="1"/><!-- keep this at value 1, and vary using CFR --> <!-- if set to 1, then all who survive get treatment but the model reports treatment in all cases -->
		  <primaquine>
			<pHumanCannotReceive value="0.0"/>
			<!-- Proportion of people who are excluded from primaquine. Do not change this value when changing health system -->
			<pUseUncomplicated value="0.95"/>
			<effectivenessOnUse value="0.775"/><!-- 1 - 0.18 / 0.8 -->
			<!-- Effectiveness of primaquine to clear hypnozoites from the liver: Chamchod and Beier 2013 cite Baird and Hoffman 2004 and give the following estimates: Probability of carrying hypnozoites after a complete course of treatments without primaquine (tropically) 0.63, with primaquine (tropically) 0.18. The value 0.8 comes from the distribution of hypnozoites, if base = 0.8, 20 percent of the broods have zero hypnozoites, so these are already without hypnozoites -->
		  </primaquine>
		</ImmediateOutcomes>
		<CFR>
		  <!-- CFR=1 so that all severe cases result in death -->
		  <!-- suggest: set to 1 minus pTreatment for severe (e.g. 1-0.48) (but note that reported nTreatments3 are treat-and-survive plus death outcomes (even without treatment))-->
		  <group lowerbound="0" value="0.52"/>
		</CFR>
		<pSequelaeInpatient>
		  <!-- no sequelae -->
		  <group lowerbound="0.0" value="0"/>
		</pSequelaeInpatient>
      </timedDeployment>
    </changeHS>
  </interventions>
  <healthSystem>
    <ImmediateOutcomes name="Perfect drugs, varying access" useDiagnosticUC="true">
      <drugRegimen firstLine="ACT" inpatient="QN" secondLine="ACT"/>
      <initialACR>
        <ACT value="1"/><!-- what is cure rate of blood-stage drugs? -->
        <QN value="1"/><!-- NEW: set to 1 OLD: no treatment for severe (can change so long as CFR=1) -->
        <selfTreatment value="0"/>
      </initialACR>
      <compliance>
        <ACT value="1"/>
        <QN value="1"/>
        <selfTreatment value="0"/>
      </compliance>
      <nonCompliersEffective>
        <ACT value="0"/>
        <QN value="0"/>
        <selfTreatment value="0"/>
      </nonCompliersEffective>
      <treatmentActions>
        <ACT name="clear blood-stage infections">
          <clearInfections stage="blood" timesteps="-1"/>
        </ACT>
        <QN name="clear blood-stage infections">
          <clearInfections stage="blood" timesteps="-1"/>
        </QN>
      </treatmentActions>
      <!--set as desired... -->
      <pSeekOfficialCareUncomplicated1 value="0.04"/>
      <pSelfTreatUncomplicated value="0"/>
      <pSeekOfficialCareUncomplicated2 value="0.04"/>
      <pSeekOfficialCareSevere value="1"/><!-- keep this at value 1, and vary using CFR --> <!-- if set to 1, then all who survive get treatment but the model reports treatment in all cases -->
      <primaquine>
        <pHumanCannotReceive value="0.0"/>
        <!-- Proportion of people who are excluded from primaquine. Do not change this value when changing health system -->
        <pUseUncomplicated value="0.95"/>
        <effectivenessOnUse value="0.775"/><!-- 1 - 0.18 / 0.8 -->
        <!-- Effectiveness of primaquine to clear hypnozoites from the liver: Chamchod and Beier 2013 cite Baird and Hoffman 2004 and give the following estimates: Probability of carrying hypnozoites after a complete course of treatments without primaquine (tropically) 0.63, with primaquine (tropically) 0.18. The value 0.8 comes from the distribution of hypnozoites, if base = 0.8, 20 percent of the broods have zero hypnozoites, so these are already without hypnozoites -->
      </primaquine>
    </ImmediateOutcomes>
    <CFR>
      <!-- CFR=1 so that all severe cases result in death -->
      <!-- suggest: set to 1 minus pTreatment for severe (e.g. 1-0.48) (but note that reported nTreatments3 are treat-and-survive plus death outcomes (even without treatment))-->
      <group lowerbound="0" value="0.52"/>
    </CFR>
    <pSequelaeInpatient>
      <!-- no sequelae -->
      <group lowerbound="0.0" value="0"/>
    </pSequelaeInpatient>
  </healthSystem>
  <entomology mode="dynamic" name="Namawala" scaledAnnualEIR="@scaledAnnualEIR@">
    <vector>
      <anopheles mosquito="gambiae_ss_indoor" propInfected="0.078" propInfectious="0.021">
        <seasonality annualEIR="@gambiae_ss_indoor_annualEIR@" input="EIR">
          <fourierSeries EIRRotateAngle="0">
            <coeffic a="1.930874053" b="1.202622093"/>
            <coeffic a="1.202622093" b="-0.896509152"/>
          </fourierSeries>
        </seasonality>
        <mosq minInfectedThreshold="0.001">
          <mosqRestDuration value="3"/>
          <extrinsicIncubationPeriod value="9"/>
          <mosqLaidEggsSameDayProportion value="0.313"/>
          <mosqSeekingDuration value="0.33"/>
          <mosqSurvivalFeedingCycleProbability value="0.623"/>
          <availabilityVariance value="0"/>
          <mosqProbBiting mean="0.95" variance="0"/>
          <mosqProbFindRestSite mean="0.95" variance="0"/>
          <mosqProbResting mean="0.99" variance="0"/>
          <mosqProbOvipositing value="0.88"/>
          <mosqHumanBloodIndex value="@mosqHumanBloodIndex@"/>
        </mosq>
        <!-- <simpleMPD>-->
        <!-- Values are made up, not from field data! -->
        <!-- <developmentDuration value="11"/>-->
        <!-- <developmentSurvival value="0.6"/>-->
        <!-- <femaleEggsLaidByOviposit value="50"/>-->
        <!-- </simpleMPD>-->
        <nonHumanHosts name="unprotectedAnimals">
          <mosqRelativeEntoAvailability value="1.0"/>
          <mosqProbBiting value="0.95"/>
          <mosqProbFindRestSite value="0.95"/>
          <mosqProbResting value="0.99"/>
        </nonHumanHosts>
      </anopheles>
      <anopheles mosquito="gambiae_ss_outdoor" propInfected="0.078" propInfectious="0.021">
        <seasonality annualEIR="@gambiae_ss_outdoor_annualEIR@" input="EIR">
          <fourierSeries EIRRotateAngle="0">
            <coeffic a="1.930874053" b="1.202622093"/>
            <coeffic a="1.202622093" b="-0.896509152"/>
          </fourierSeries>
        </seasonality>
        <mosq minInfectedThreshold="0.001">
          <mosqRestDuration value="3"/>
          <extrinsicIncubationPeriod value="9"/>
          <mosqLaidEggsSameDayProportion value="0.313"/>
          <mosqSeekingDuration value="0.33"/>
          <mosqSurvivalFeedingCycleProbability value="0.623"/>
          <availabilityVariance value="0"/>
          <mosqProbBiting mean="0.95" variance="0"/>
          <mosqProbFindRestSite mean="0.95" variance="0"/>
          <mosqProbResting mean="0.99" variance="0"/>
          <mosqProbOvipositing value="0.88"/>
          <mosqHumanBloodIndex value="@mosqHumanBloodIndex@"/>
        </mosq>
        <!-- <simpleMPD>-->
        <!-- Values are made up, not from field data! -->
        <!-- <developmentDuration value="11"/>-->
        <!-- <developmentSurvival value="0.6"/>-->
        <!-- <femaleEggsLaidByOviposit value="50"/>-->
        <!-- </simpleMPD>-->
        <nonHumanHosts name="unprotectedAnimals">
          <mosqRelativeEntoAvailability value="1.0"/>
          <mosqProbBiting value="0.95"/>
          <mosqProbFindRestSite value="0.95"/>
          <mosqProbResting value="0.99"/>
        </nonHumanHosts>
      </anopheles>
      <nonHumanHosts name="unprotectedAnimals" number="1.0"/>
    </vector>
  </entomology>
</om:scenario>
